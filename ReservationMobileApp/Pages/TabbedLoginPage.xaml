            xmlns:tabbedPages="clr-namespace:ReservationMobileApp.Views"
            x:Class="ReservationMobileApp.Pages.TabbedLoginPage"
             xmlns:android="clr-namespace:Xamarin.Forms.PlatformConfiguration.AndroidSpecific;assembly=Xamarin.Forms.Core"
            android:TabbedPage.ToolbarPlacement="Bottom"
            >
    
    <!--Use to combine two pages as tabbed page-->

    <tabbedPages:LoginView Title="User Login" IconImageSource="ic_home_black_24dp"></tabbedPages:LoginView>
    <tabbedPages:LoginServiceProviderView Title="Service Provider Login" IconImageSource="ic_home_black_24dp"></tabbedPages:LoginServiceProviderView>
</TabbedPage>
